# LLM Provider: anthropic, openai, gemini, or ollama
LLM_PROVIDER=anthropic

# API key for your chosen LLM provider (not needed for ollama)
LLM_API_KEY=your-api-key-here

# Optional: override the default model for your provider
# LLM_MODEL=

# Tavily API key for web search tool
SEARCH_API_KEY=your-tavily-api-key-here

# Flask secret key (change in production)
# SECRET_KEY=your-secret-key-here
